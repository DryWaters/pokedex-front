{
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended"
    ],
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "settings": {
      "react": {
        "pragma": "React",  // Pragma to use, default to "React"
        "version": "latest"
      }
    },
    "env": {
      "browser": true,
      "jest": true,
      "es6": true
    }
}
